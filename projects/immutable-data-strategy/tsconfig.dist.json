        "baseUrl": ".",
        "declaration": true,
        "stripInternal": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "es2015",
        "moduleResolution": "node",
        "noEmitOnError": false,
        "outDir": "./release",
        "rootDir": ".",
        "sourceMap": true,
        "inlineSources": true,
        "lib": [
            "es2016",
            "dom"
        ],
        "target": "es5",
        "skipLibCheck": true,
        "paths": {
            "@react-state/data-strategy": [
                "../data-strategy/release"
            ]
        }
    },
    "files": [
        "index.ts"
    ]
}